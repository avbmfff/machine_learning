a":{"text/plain":["array([ 3.54813977e-02,  1.29391098e-01,  9.12675046e-02,  9.93821732e+01,\n","        5.18440648e-01,  5.80262883e-02,  1.74421518e-03,  2.54889569e-02,\n","        1.24477518e-01,  3.07130321e-01,  8.14101969e+01,  1.79719763e+01,\n","        6.97817480e+01,  6.20826761e+00,  1.69067460e+00, -4.33746213e+00])"]},"execution_count":31,"metadata":{},"output_type":"execute_result"}],"source":["import numpy as np\n","\n","def mse(y_true, y_pred):\n","    return np.nan_to_num(np.mean((y_true - y_pred)**2))\n","\n","def grad_mse(X, y_true, y_pred):\n","    n = len(y_true)\n","    return -2/n * np.dot(X.T, (y_true - y_pred))\n","\n","def update_params(params, learning_rate, grad):\n","    return params - learning_rate * grad\n","\n","def add_intercept(X):\n","    return np.concatenate((np.ones((X.shape[0], 1)), X), axis=1)\n","\n","def fit_linear_regression(X, y, learning_rate=0.01, num_iterations=1000, regularization=None, l1_ratio=None):\n","    X = add_intercept(X)\n","    params = np.zeros(X.shape[1])\n","    losses = []\n","\n","    for i in range(num_iterations):\n","        y_pred = np.dot(X, params)\n","        loss = mse(y, y_pred)\n","        losses.append(loss)\n","\n","        grad = grad_mse(X, y, y_pred)\n","\n","        if regularization == 'l2':\n","            grad += 2 * l1_ratio * params\n","        elif regularization == 'l1':\n","            grad += l1_ratio * np.sign(params)\n","        elif regularization == 'elastic_net':\n","            grad += (l1_ratio * np.sign(params) + (1 - l1_ratio) * 2 * params)\n","\n","        params = update_params(params, learning_rate, grad)\n","\n","    return params, losses\n","\n","training_points = training_points.dropna()\n","training_values = training_values.dropna()\n","\n","params, losses = fit_linear_regression(training_points.values, training_values.values, learning_rate=0.00000000001, num_iterations=10000, regularization='l2', l1_ratio=0.5)\n","params"]},{"cell_type":"markdown","metadata":